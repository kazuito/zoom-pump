{
  "manifest_version": 3,
  "name": "ZoomPump",
  "version": "0.0.1",
  "action": {
    "default_title": "ZoomPump",
    "default_popup": "./popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "options_page": "./options/options.html"
}
